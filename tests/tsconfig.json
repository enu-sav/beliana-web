{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node",
      "cypress-plugin-steps"
    ],
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts"
  ]
}
