{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/migrate_source_yaml",
                "version": "dev-custom",
                "type": "drupal-module",
                "source": {
                    "type": "git",
                    "url": "https://git.drupal.org/sandbox/stborchert/2808617.git",
                    "reference": "8.x-1.x"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "~1.0",
        "drupal/admin_toolbar": "^3.1",
        "drupal/adminimal_admin_toolbar": "~1.0",
        "drupal/adminimal_theme": "~1.0",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/captcha": "^1.3",
        "drupal/color-color": "^1.0",
        "drupal/colorbox": "^1.10",
        "drupal/config_filter": "^2.3",
        "drupal/config_ignore": "~2.0",
        "drupal/config_split": "^2.0@RC",
        "drupal/console": "~1.0",
        "drupal/core": "^9.4",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/ctools": "^3.7",
        "drupal/easy_breadcrumb": "2.x-dev@dev",
        "drupal/entity": "^1.3",
        "drupal/entity_browser": "^1.0",
        "drupal/entityqueue": "1.x-dev",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/facets": "^2.0",
        "drupal/fontawesome": "^2.23",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/mathjax": "^3.0",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/menu_link_attributes": "~1.0",
        "drupal/metatag": "^1.19",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/page_manager": "^4.0",
        "drupal/paragraphs": "^1.13",
        "drupal/pathauto": "^1.10",
        "drupal/purge": "^3.2",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.7",
        "drupal/search_api": "^1.23",
        "drupal/search_api_solr": "^4.2",
        "drupal/simple_sitemap": "^3.11",
        "drupal/svg_image": "^1.16",
        "drupal/token": "^1.10",
        "drupal/ultimate_cron": "^2.0",
        "drupal/upgrade_status": "^3.14",
        "drupal/varnish_purge": "^2.1",
        "drupal/views_infinite_scroll": "^1.9",
        "drupal/views_natural_sort": "^2.0",
        "drush/drush": "^10",
        "symfony/filesystem": "^3.4",
        "symfony/finder": "^3.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/admin_toolbar": {
                "3121816 - Version 8.x-2.1 need Drupal 8.8.0 or greater": "https://www.drupal.org/files/issues/2020-03-30/3121816-07.patch"
            },
            "drupal/purge": {
                "3259320 - PHP 8.1 compatibility : fix deprecated warnings": "https://www.drupal.org/files/issues/2022-02-24/3259320-10.patch"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true
        }
    }
}
